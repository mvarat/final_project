<h2>Sign Up</h2>

<%= form_for @user do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password%>

  <%= f.label :password_confirmation, "Password Confirmtaion" %>
  <%= f.password_field :password_confirmation %>
  <br>
  <%= f.submit %>
<% end %>
