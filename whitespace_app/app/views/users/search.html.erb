<h3>search artwork</h3>
<div ng-controller="ArtworksController">
  <form>
    <input placeholder="Search" ng-model="term">
    <input type="button" class="btn" ng-click="search(term)" value="SEARCH">
  </form>

  <div ng-repeat="artwork in artworks">
    <!-- {{ artwork }} -->
    <!-- <br>
    <button name="button" ng-click="getArtwork()">GIVE ME ART!</button>
    <br> -->

    <img src= "{{artwork.thumbnailImage}}">
    <p>{{artwork.title}}</p>
    <button name="button" ng-click="saveArt(artwork)">SAVE ART</button>
  </div>
</div>
